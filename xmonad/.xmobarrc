Config { font = "-misc-fixed-*-*-*-*-13-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = TopW L 90 
       , commands = [ 
                     Run Cpu ["-L","15","-H","50","--normal","green","--high","red"] 10
                    , Run Date "%a %b %_d %Y %l:%M%p" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %cpu% |  %date%    "
       }  
